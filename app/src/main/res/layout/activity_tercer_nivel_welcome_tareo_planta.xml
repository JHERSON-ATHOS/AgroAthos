<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/clPrincipalRRHH_TAREO_AR_PLANTA_TERCER_NIVEL"
    tools:context=".RRHH_TAREO_AR_PLANTA.TercerNivelWelcomeTareoPlanta">

    <Spinner
        android:id="@+id/spProcesoRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50sp"
        android:layout_marginEnd="30dp"
        android:paddingRight="30dp"
        android:background="@drawable/spinner_style_rrhhtareoar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/item_spinner_datos_rrhhtareoar" />

    <Spinner
        android:id="@+id/spActividadRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="30dp"
        android:paddingRight="30dp"
        android:background="@drawable/spinner_style_rrhhtareoar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spProcesoRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        tools:listitem="@layout/item_spinner_datos_rrhhtareoar" />

    <Spinner
        android:id="@+id/spLaborRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="30dp"
        android:paddingRight="30dp"
        android:background="@drawable/spinner_style_rrhhtareoar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spActividadRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        tools:listitem="@layout/item_spinner_datos_rrhhtareoar" />

    <Spinner
        android:id="@+id/spMesaRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="30dp"
        android:paddingRight="30dp"
        android:background="@drawable/spinner_style_rrhhtareoar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spLaborRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        tools:listitem="@layout/item_spinner_datos_rrhhtareoar" />

    <Button
        android:id="@+id/btnRegistrarRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:text="Registrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spMesaRRHH_TAREO_ARANDANO_PLANTA_TERCER_NIVEL"
        app:layout_constraintVertical_bias="0.482" />

</androidx.constraintlayout.widget.ConstraintLayout>